<h1>Publisher</h1>
<%= form_for(@publisher, url: admin_publishers_path) do |f| %>
  <div class="field">
  	<%= f.label :name %>
  	<%= f.text_field :name %>
  </div>
  
  <div class="action">
  	<%= f.submit "Create"%>
  </div>
  <p>Back to <%= link_to "Admin Page", admin_index_path %></p>
<% end %>